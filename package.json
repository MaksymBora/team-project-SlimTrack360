    "emotion-normalize": "^11.0.1",
    "faker": "^5.5.3",
    "flowbite": "^2.2.0",
    "flowbite-react": "^0.7.0",
    "formik": "^2.4.5",
    "keyframes": "^2.3.0",
    "modern-normalize": "^2.0.0",
    "nanoid": "^5.0.4",
    "normalize.css": "^8.0.1",
    "npm": "^10.2.5",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.2",
    "react-responsive": "^9.0.2",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.15.0",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.3",
